<h1>Listing languages</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Level</th>
  </tr>

<% @languages.each do |language| %>
  <tr>
    <td><%=h language.title %></td>
    <td><%=h language.description %></td>
    <td><%=h language.level %></td>
  </tr>
<% end %>
</table>

<br />
