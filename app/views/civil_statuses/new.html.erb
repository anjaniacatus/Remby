<% title  "Create civil status " %>
<% semantic_form_for (@civil_status, :html => {:multipart => true}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= f.file_field :image_file %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', civil_statuses_path %>
