<% title " All CVS " %>
<%= render @cvs %>
  <%- unless current_user.blank? %>
    <%-if current_user.roles == "member"%>
      <%= link_to 'Back', civil_status_path(@civil_status)%>
      <%= link_to 'New cv', new_civil_status_cv_path(@civil_status) %>
    <% end %>  
    <%- if current_user.roles == "compagny" %>
      <%= link_to "back", compagny_path(current_user.compagny)%>
    <% end %>
<% end %>