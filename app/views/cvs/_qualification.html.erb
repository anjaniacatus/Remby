  </p>
  <p>
    Qualification <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    Qualification <%= f.label :institute %><br />
    <%= f.text_field :institute %>
  </p>
  <p>
    Qualification <%= f.label :duration %><br />
    <%= f.text_field :duration %>
  </p>
  <p>
    Qualification <%= f.label :start_on %><br />
    <%= f.date_select :start_on %>
  </p>
  <p>
    Qualification <%= f.label :end_on %><br />
    <%= f.date_select :end_on %>
