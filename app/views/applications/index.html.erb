<h1>Listing applications</h1>

<table>
  <tr>
    <th>Job</th>
    <th>Cv</th>
    <th>Title</th>
  </tr>

<% @applications.each do |application| %>
  <tr>
    <td><%=h application.job_id %></td>
    <td><%=h application.title %></td>
    <td><%= link_to 'Show', application %></td>
  </tr>
<% end %>
</table>

<br />
