- semantic_form_for @user do |f|
  = f.error_messages
  %p
    = f.label :username
    %br/
    = f.text_field :username
  %p
    = f.label :email
    %br/
    = f.text_field :email
  %p
    = f.label :password
    %br/
    = f.password_field :password
    %br
    = f.label :password_confirmation
    %br/
    = f.password_field :password_confirmation
  %p
    
    = f.input :roles, :collection => ["member", "compagny"]
  %p
    = f.submit "Submit"
    
