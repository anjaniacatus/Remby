<h1>New qualification</h1>

<% form_for(@qualification) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :institute %><br />
    <%= f.text_field :institute %>
  </p>
  <p>
    <%= f.label :duration %><br />
    <%= f.text_field :duration %>
  </p>
  <p>
    <%= f.label :start_on %><br />
    <%= f.date_select :start_on %>
  </p>
  <p>
    <%= f.label :end_on %><br />
    <%= f.date_select :end_on %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', qualifications_path %>