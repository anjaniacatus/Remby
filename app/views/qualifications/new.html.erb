<h1>New qualification</h1>

<% form_for([@qualification.profile, @qualification]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :school %><br />
    <%= f.text_field :school %>
  </p>
  <p>
    <%= f.label :obtain_at %><br />
    <%= f.date_select :obtain_at %>
  </p>
   <p>
    <%= f.label :study_duration %><br />
    <%= f.text_field :study_duration %>
   </p>
   <p>
    <%= f.label :permalink %><br />
    <%= f.text_field :permalink %>
   </p>


  <p>
    <%= f.label :comment %><br />
    <%= f.text_area :comment %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', qualifications_path %>