<%= f.error_messages %>

<p>
  <%= f.input :title, :label => "Titre" %>
</p>

<p>
  <%= f.text_area :summary, :cols => "50", :rows => "3" %>
</p>

<div id='degree_course_children'>
  <% f.fields_for :degree_courses do |degree_course_form| %>
    <%= render :partial => 'degree_course', :locals => { :f => degree_course_form } %>
  <% end %>
</div>

<p>
  <%= add_child_link 'Add a DegreeCourse', 'degree_course', f %>
</p>

