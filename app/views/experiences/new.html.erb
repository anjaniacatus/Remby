<h1>New experience</h1>

<% form_for(@experience) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :permalink %><br />
    <%= f.text_field :permalink %>
  </p>
  <p>
    <%= f.label :office_name %><br />
    <%= f.text_field :office_name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :society_name %><br />
    <%= f.text_field :society_name %>
  </p>
  <p>
    <%= f.label :period %><br />
    <%= f.text_field :period %>
  </p>
  <p>
    <%= f.label :begin_at %><br />
    <%= f.date_select :begin_at %>
  </p>
  <p>
    <%= f.label :profile %><br />
    <%= f.text_field :profile %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', experiences_path %>